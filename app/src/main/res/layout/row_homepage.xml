<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homepageRowModel" type="com.tourpost.app.modules.homepage.data.model.HomepageRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowdelhi"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <TextView android:ellipsize="end" android:id="@+id/txtDelhi"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_weight="0.36" android:layout_width="0dp"
            android:singleLine="true"
            android:text="@{homepageRowModel.txtDelhi}"
            style="@style/txtLeagueSpartanmedium12" tools:text="@string/lbl_delhi"/>
        <LinearLayout android:id="@+id/linearRowchat"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_213pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_weight="0.64" android:layout_width="0dp" android:orientation="horizontal">
            <ImageView android:id="@+id/imageChat"
                android:layout_height="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_10pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_chat"
                tools:ignore="ContentDescription" tools:src="@drawable/img_chat"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTwenty"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginStart="@dimen/_28pxh"
                android:layout_weight="0.44" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{homepageRowModel.txtTwenty}"
                style="@style/txtLeagueSpartanregular8" tools:text="@string/lbl_20"/>
        </LinearLayout>
    </LinearLayout>
</layout>
